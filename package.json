{
  "name": "photo-viewer-deskapp",
  "version": "0.1.0",
  "private": true,
  "author": "Wong Lok @wonglok831",
  "description": "a beautiful photo viewing app",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build-vue": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "icons": "electron-icon-builder --input=./public/icon.png --output=build --flatten",
    "deploy-to-github": "git add .; git commit -am \"git backup\"; git push -u origin master",
    "deploy-m": "npm run build; npm run vercel;",
    "deploy": "yarn deploy-to-github; yarn electron:build -p always",
    "dev-o2": "vue-cli-service serve --port 8080",
    "dev-o": "vue-cli-service serve --port $PORT",
    "devs": "vue-cli-service serve --port 8080 --https",
    "dev": "vue-cli-service electron:serve",
    "build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "netlify": "netlify deploy --prod --dir=\"./dist\";",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "vercel": "vercel --prod"
  },
  "main": "background.js",
  "dependencies": {
